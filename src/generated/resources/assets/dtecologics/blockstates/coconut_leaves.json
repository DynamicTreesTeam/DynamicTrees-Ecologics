{
  "multipart": [
    {   "when": { "distance": "1|2" },
      "apply": { "model": "dtecologics:block/coconut_frond" }
    },
    {   "when": { "distance": "3" },
      "apply": { "model": "dtecologics:block/coconut_core_top" }
    },
    {   "when": { "distance": "4" },
      "apply": { "model": "dtecologics:block/coconut_core_bottom" }
    },
    {   "when": {"OR":[ { "direction": "0", "distance": "1|2" }, { "distance": "5|6|7" } ]},
      "apply": { "model": "ecologics/coconut_leaves" }
    }
  ]
}